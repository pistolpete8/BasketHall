							</div>
							<div class="option" value="5 na 5">
								5 na 5
							</div>

						</div>



					</div>


					@{
						if (Model.Count() == 0)
						{

							<h5 class="trenutnoNema text-danger"> TRENUTNO NEMA AKTUELNIH TERMINA </h5>


						}
					}
					<div class="sve box">
						<div class="sve box">
							<ul class="event-list">

								@{

                                    foreach (var termin in Model.OrderByDescending(date => date.Termin.PocetakTermina))
