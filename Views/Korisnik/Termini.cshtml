						<!--- proba-->
						<div class="drop">
							<div class="option active placeholder" data-value="placeholder">
								Vrsta basketa
							</div>
							<div class="option" value="sve">
								Svi Termini
							</div>
							<div class="option" value="3 na 3">
								3 na 3
							</div>
							<div class="option" value="4 na 4">
